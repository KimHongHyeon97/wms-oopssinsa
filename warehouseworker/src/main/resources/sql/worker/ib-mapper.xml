<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "https://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.oopssinsa.model.dao.IbMapper">

    <select id="findIb" resultType="com.oopssinsa.model.dto.IbDto">
        select *
        from ib_detail
        where id = #{id} and manufacture_id = #{manufactureId} and product_id = #{productId}
    </select>

</mapper>